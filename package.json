{
  "name": "reveal.js-xapi-plugin",
  "version": "0.0.0",
  "description": "Plugin for xAPI",
  "main": "index.js",
  "scripts": {
    "dev:docserver": "http-server ./localdev/xapidocs/",
    "dev:plugin": "webpack --config ./webpack.config.js -w",
    "dev:reveal:start": "./node_modules/.bin/reload -p 8084 -d ./revealJS/",
    "dev": "npm start",
    "dist": "./node_modules/.bin/rimraf ./revealJS/plugin/xapi/xapi.js & webpack --config ./webpack.config.js",
    "preinstall": "bash ./scripts/get-reveal.sh",
    "start": "npm run dev:plugin & npm run dev:reveal:start",
    "test": "ava",
    "test:ones": "ava",
    "test:watch": "ava --watch"
  },
  "keywords": [
    "reveal.js",
    "xapi",
    "plugin"
  ],
  "author": "Garrick Lee <garrick@oslid.com>",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.9",
    "http-server": "^0.11.1",
    "local-web-server": "^1.2.7",
    "nodemon": "^1.11.0",
    "preact": "^8.2.7",
    "pug": "^2.0.0-beta11",
    "ramda": "^0.23.0",
    "reload": "^2.2.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.1",
    "tincanjs": "^0.50.0",
    "webpack": "^2.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "jsonld": "^0.5.18"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
