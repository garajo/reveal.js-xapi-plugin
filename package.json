    "babel-preset-es2017": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "local-web-server": "^1.2.7",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-beta11",
    "ramda": "^0.23.0",
    "rimraf": "^2.6.2",
    "tincanjs": "^0.50.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "fetch": "^1.1.0",
